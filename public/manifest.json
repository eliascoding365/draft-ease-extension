{
    "manifest_version": 3,
    "name": "Note Ease",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "description": "Create and save notes easily.",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}